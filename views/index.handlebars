<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <div id="searchBarGroup" class="input-group mb-3">
        <div class="form-group">
          <input type="text" id="searchInput" class="form-control width-control" name="cellnumber" placeholder="(555) 555-5555" required="required">
        </div>
        <div class="form-group">
          <button type="submit" id="searchSubmit" class="btn btn-primary btn-block btn-lg">search</button>
        </div>
      </div>
    </div>
  </div>
</div>

<ul id="example-list" class="list-group">
  {{#each keepInTouch}}
  <li data-id="{{this.id}}" class="list-group-item">
    <a href="example/{{this.id}}">{{this.text}}</a>
    <button class="btn btn-danger float-right delete">ï½˜</button>
  </li>
  {{/each}}
</ul>
